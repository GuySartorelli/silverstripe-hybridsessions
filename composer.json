{
    "name": "silverstripe/hybridsessions",
    "description": "Cookie/DB session support for SilverStripe",
    "type": "silverstripe-module",
    "keywords": [
        "silverstripe",
        "session",
        "cwp"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Hamish Friedlander",
            "email": "hamish@silverstripe.com"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "silverstripe/versioned": "^1@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\HybridSessions\\": "src/",
            "SilverStripe\\HybridSessions\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
